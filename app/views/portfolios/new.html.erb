<h1>Create a new portfolio item</h1>
<%= form_for(@portfolio_item) do |f| %>
  <div class="field">
    <%= form.label :title %>
    <%= form.text_field :title%>
  </div>

  <div class="field">
    <%= form.label :body %>
    <%= form.text_area :body%>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>